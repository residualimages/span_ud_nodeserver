<editors>
	<!-- Boolean -->
	<editor id="bool">
		<range uom="2" subset="0,1" />
	</editor>
	<editor id="raw">
		<range uom="56" min="0" max="20000" prec="0" /> 
	</editor>
	<editor id="watt">
		<range uom="73" min="-24000" max="24000" prec="2" /> 
	</editor>
	<editor id="timestamp">
		<range uom="151" min="0" max="2147483647" prec="0"/> 
	</editor>
	<editor id="string">
		<range uom="145" min="0" max="0" prec="0"/> 
	</editor>
	<editor id="panelgridstatus">
		<range min="1" max="2" step="1" prec="0" uom="25">
			<nls>
			<e id="1">PANEL_ON_GRID</e>
			<e id="2">UNKNOWN STATE</e>				
			</nls>
		</range>
	</editor>
	<editor id="circuitstatus">
		<range min="-1" max="1" step="1" prec="0" uom="25">
			<nls>
			<e id="-1">Unknown</e>
			<e id="0">Breaker Open or Tripped (NOT Supplying Power)</e>
			<e id="1">Breaker Closed (Supplying Power)</e>				
			</nls>
		</range>
	</editor>
</editors>
