<nodeDefs>

  <nodeDef id="ctl" nls="ctl">
    <sts>
      <st id="ST" editor="bool" />
      <st id="GV0" editor="raw" />
	</sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="RESET" />
      </accepts>
    </cmds>
  </nodeDef>

  <nodeDef id="panelForCircuits" nls="panelForCircuits">
	  <editors>
		<editor id="SPAN_PANELGRIDSTATUS">
			<range uom="25" subset="1-3" nls="IX_SPAN_PANELGRIDSTATUS" />
		</editor>
	  </editors>
    <sts>
      <st id="ST" editor="watt" />
      <st id="FREQ" editor="string" />
      <st id="PULSCNT" editor="raw" />
      <st id="CLIEMD" editor="SPAN_PANELGRIDSTATUS" />
      <st id="TIME" editor="timestamp" />
      <st id="TIMEREM" editor="string" />
      <st id="AWAKE" editor="bool" />
	  </sts>
    <cmds>
      <sends />
	    <accepts>
        <cmd id="TOGGLE_MONITORING">
          <p id="" editor="bool" init="AWAKE" />
        </cmd>
      </accepts>
    </cmds>
  </nodeDef>
	
  <nodeDef id="circuit" nls="circuit">
	  <editors>
		<editor id="SPAN_CIRCUITSTATUS">
			<range uom="25" subset="0-3" nls="IX_SPAN_CIRCUITSTATUS" />
		</editor>
	  </editors>
    <sts>
      <st id="ST" editor="watt" />
      <st id="PULSCNT" editor="raw" />
      <st id="CLIEMD" editor="SPAN_CIRCUITSTATUS" />
      <st id="TIME" editor="timestamp" />
      <st id="TIMEREM" editor="string" />
      <st id="AWAKE" editor="bool" />
      <st id="GPV" editor="string" />
      <st id="GV1" editor="raw" />
      <st id="GV2" editor="raw" />
      <st id="GV3" editor="raw" />
      <st id="GV4" editor="raw" />
	  </sts>
    <cmds>
      <sends />
	    <accepts>
        <cmd id="TOGGLE_CIRCUIT_MONITORING">
          <p id="" editor="bool" init="AWAKE" />
        </cmd>
      </accepts>
    </cmds>  
  </nodeDef>
	
  <nodeDef id="panelForBreakers" nls="panelForBreakers">
    <sts>
      <st id="ST" editor="watt" />
      <st id="FREQ" editor="string" />
      <st id="PULSCNT" editor="raw" />
      <st id="GPV" editor="raw" />
      <st id="TIME" editor="timestamp" />
      <st id="TIMEREM" editor="string" />
	  </sts>
  </nodeDef>
	
  <nodeDef id="breaker" nls="breaker">
	  <editor>
		<editor id="SPAN_BREAKERSTATUS">
			<range uom="25" subset="0-3" nls="IX_SPAN_BREAKERSTATUS" />
		</editor>
	  </editors>
    <sts>
      <st id="ST" editor="watt" />
      <st id="PULSCNT" editor="raw" />
      <st id="CLIEMD" editor="SPAN_BREAKERSTATUS" />
      <st id="TIME" editor="timestamp" />
      <st id="TIMEREM" editor="string" />
	  </sts>
  </nodeDef>
</nodeDefs>
